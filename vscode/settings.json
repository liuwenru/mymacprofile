{
  "update.mode": "manual",
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.colorTheme": "Atom One Dark",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.enablePreview": false,
  "window.zoomLevel": 1,
  "editor.fontFamily": "'SourceCodePro-Regular', Monaco, 'Courier New'",
  "editor.rename.enablePreview": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit",
    "source.fixAll": "explicit"
  },
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "terminal.integrated.fontFamily": "'SourceCodePro-Regular', Monaco, 'Courier New'",
  "terminal.integrated.defaultProfile.windows": "powershell.exe",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.enablePersistentSessions": false,
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.tabs.title": "${cwdFolder}${separator}${process}${separator}${task}",
  "terminal.integrated.tabs.description": "${task}${separator}${local}${separator}${process}",
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "zsh",
      "args": ["-l"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
      // "args": ["attach"]
    }
  },
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "zsh",
      "args": ["-l", "-i"]
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
      // "args": ["attach"]
    }
  },
  "terminal.integrated.inheritEnv": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "window.titleBarStyle": "custom",
  "window.restoreWindows": "none",
  "workbench.colorCustomizations": {
    "minimap.background": "#00002299",
    "editor.selectionHighlightBackground": "#56554b" // 设置选中时相同单词的背景颜色
  },
  "debug.console.fontSize": 13,
  "bookmarks.saveBookmarksInProject": true,
  "bookmarks.useWorkaroundForFormatters": true,
  "bookmarks.experimental.enableNewStickyEngine": true,
  "bookmarks.keepBookmarksOnLineDelete": true,
  "bookmarks.sideBar.expanded": true,
  "editor.wordWrap": "on",
  "editor.detectIndentation": false,
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.semanticHighlighting.enabled": true,
  "security.workspace.trust.untrustedFiles": "open",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true,
    "**/.svn": true,
    "/root/share": true,
    "/root/tempwork": true
  },
  // 文件路径自动补全插件
  "path-intellisense.extensionOnImport": true,
  "path-intellisense.showHiddenFiles": true,
  "path-intellisense.absolutePathToWorkspace": true,
  "prettier.useTabs": false,
  "prettier.tabWidth": 2,
  "prettier.endOfLine": "lf",
  // git 配置
  "git.ignoreLegacyWarning": true,
  "git.allowForcePush": false,
  "git.autofetch": "all",
  "git.openRepositoryInParentFolders": "always",
  // golang 配置
  "go.useLanguageServer": true,
  "go.toolsManagement.autoUpdate": true,
  // python 配置
  "python.languageServer": "Pylance",
  // java 配置
  "java.import.generatesMetadataFilesAtProjectRoot": true,
  "java.project.importOnFirstTimeStartup": "automatic",
  "maven.terminal.useJavaHome": true,
  "java.format.enabled": false,
  "java.saveActions.organizeImports": false,
  "java.project.referencedLibraries": ["lib/**/*.jar"],
  // CMake 配置
  "cmake.configureOnOpen": true,
  "redhat.telemetry.enabled": false,
  "Lua.telemetry.enable": false,
  "editor.foldingMaximumRegions": 65000,
  // 退出是否提示确认
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "path-intellisense.autoSlashAfterDirectory": true,
  "path-intellisense.autoTriggerNextSuggestion": true,
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.linux": "~/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
