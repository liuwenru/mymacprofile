    "vscode-kubernetes.helm-path-mac": "/Users/liuwenru/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.kubectl-path-mac": "/Users/liuwenru/.vs-kubernetes/tools/kubectl/kubectl",
    "vscode-kubernetes.minikube-path-mac": "/Users/liuwenru/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
  },
  //  二进制编辑器插件配置
  "hexeditor.columnWidth": 4,
  "hexeditor.showDecodedText": true,
  "hexeditor.defaultEndianness": "little",
  "hexeditor.inspectorType": "aside",
  // 代办插件
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "epoint"
